@page
@model ViewModel
@{
  ViewData["Title"] = "Administrator Page";
}
@{
  ViewBag.articles = ViewData["Articles"];
  var x = ViewBag.articles;
  @x.judul
  @x.publisher
  <br>
  <br>
  ViewBag.comment = ViewData["Comment"];
  var z = ViewBag.comment;
  foreach (var i in z)
  {
    <h5>@i.username</h5>
    <h5>@i.comment</h5>
  }
  <br>
  <br>
  ViewBag.user = ViewData["User"];
  var y = ViewBag.user;
  if(y != null)
  {
  <form method="POST">
    <input type="hidden" name="id" value="@x.id">
    <input type="hidden" name="user" value="@y">
    <input type="text" name="comment" value="">
    <input type="submit" class="btn btn-success">
  </form>
  }
}